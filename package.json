{
  "name": "@sabertazimi/blog",
  "version": "2.4.3",
  "description": "A blog system based on React and Gatsby",
  "author": "sabertazimi",
  "license": "MIT",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "audit": "npm audit fix --registry https://registry.npmjs.org",
    "build": "gatsby build && npm run test:badge",
    "build:gh-pages": "gatsby build --prefix-paths && npm run test:badge",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css}",
    "lint": "npm run lint:style && npm run lint:type-check",
    "lint:style": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "lint:type-check": "tsc --noEmit",
    "prelease": "standard-version --dry-run -s",
    "release": "standard-version -s",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "start:watch": "concurrently \"npm:develop\" \"npm:test:watch\"",
    "test": "jest -o",
    "test:all": "jest",
    "test:badge": "node scripts/generateJestCoverageBadge.js",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@octokit/rest": "^18.9.0",
    "@reach/router": "^1.3.4",
    "antd": "^4.16.11",
    "classnames": "^2.3.1",
    "gatsby": "^3.11.1",
    "prismjs": "^1.24.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-spring": "^9.2.4",
    "tailwindcss": "^2.2.7",
    "typed.js": "^2.0.12"
  },
  "devDependencies": {
    "@octokit/types": "^6.23.0",
    "@react-spring/types": "^9.2.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.13",
    "@types/prismjs": "^1.16.6",
    "@types/reach__router": "^1.3.9",
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "autoprefixer": "^10.3.1",
    "babel-jest": "^27.0.6",
    "babel-preset-gatsby": "^1.11.0",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.4.3",
    "gatsby-plugin-antd": "^2.2.0",
    "gatsby-plugin-catch-links": "^3.11.0",
    "gatsby-plugin-eslint": "^3.0.0",
    "gatsby-plugin-manifest": "^3.11.0",
    "gatsby-plugin-minify-html": "^1.0.4",
    "gatsby-plugin-offline": "^4.11.0",
    "gatsby-plugin-postcss": "^4.11.0",
    "gatsby-plugin-react-helmet": "^4.11.0",
    "gatsby-plugin-robots-txt": "^1.6.8",
    "gatsby-plugin-sitemap": "^4.7.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.24",
    "gatsby-plugin-zopfli": "^2.0.0",
    "gatsby-remark-autolink-headers": "^4.8.0",
    "gatsby-remark-copy-linked-files": "^4.8.0",
    "gatsby-remark-prismjs": "^5.8.0",
    "gatsby-remark-smartypants": "^4.8.0",
    "gatsby-source-filesystem": "^3.11.0",
    "gatsby-transformer-remark": "^4.8.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "node-fetch": "^2.6.1",
    "postcss": "^8.3.6",
    "postcss-import": "^14.0.2",
    "prettier": "^2.3.2",
    "react-test-renderer": "^16.14.0",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.4",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sabertazimi/blog"
  },
  "bugs": {
    "url": "https://github.com/sabertazimi/blog/issues"
  }
}
