{
  "name": "@sabertazimi/blog",
  "version": "1.0.0",
  "description": "A blog system based on React and Gatsby",
  "author": "sabertazimi",
  "license": "MIT",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "audit": "npm audit fix --registry https://registry.npmjs.org",
    "build": "gatsby build",
    "build:gh-pages": "gatsby build --prefix-paths",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "eslint": "eslint src --ignore-path .gitignore --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write src/**/*.{js,jsx,md,mdx,css}",
    "lint": "npm run eslint && npm run type-check",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@octokit/rest": "^18.7.1",
    "@reach/router": "^1.3.4",
    "antd": "^4.16.8",
    "classnames": "^2.3.1",
    "gatsby": "^3.10.1",
    "prismjs": "^1.24.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-spring": "^9.2.4",
    "tailwindcss": "^2.2.7",
    "typed.js": "^2.0.12"
  },
  "devDependencies": {
    "@octokit/types": "^6.21.1",
    "@react-spring/types": "^9.2.4",
    "@types/node": "^16.4.2",
    "@types/prismjs": "^1.16.6",
    "@types/reach__router": "^1.3.9",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "autoprefixer": "^10.3.1",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-webpack-plugin": "^3.0.1",
    "gatsby-plugin-antd": "^2.2.0",
    "gatsby-plugin-catch-links": "^3.10.0",
    "gatsby-plugin-eslint": "^3.0.0",
    "gatsby-plugin-manifest": "^3.10.0",
    "gatsby-plugin-minify-html": "^1.0.4",
    "gatsby-plugin-offline": "^4.10.0",
    "gatsby-plugin-postcss": "^4.10.0",
    "gatsby-plugin-react-helmet": "^4.10.0",
    "gatsby-plugin-robots-txt": "^1.6.8",
    "gatsby-plugin-sitemap": "^4.6.0",
    "gatsby-plugin-zopfli": "^2.0.0",
    "gatsby-remark-autolink-headers": "^4.7.0",
    "gatsby-remark-copy-linked-files": "^4.7.0",
    "gatsby-remark-prismjs": "^5.7.0",
    "gatsby-remark-smartypants": "^4.7.0",
    "gatsby-source-filesystem": "^3.10.0",
    "gatsby-transformer-remark": "^4.7.0",
    "postcss": "^8.3.6",
    "postcss-import": "^14.0.2",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sabertazimi/blog"
  },
  "bugs": {
    "url": "https://github.com/sabertazimi/blog/issues"
  }
}
