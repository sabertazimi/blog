{
  "name": "@sabertazimi/blog",
  "version": "2.12.0",
  "description": "A blog system based on React and Gatsby",
  "author": "sabertazimi",
  "license": "MIT",
  "keywords": [
    "gatsby"
  ],
  "packageManager": "yarn@3.0.2",
  "scripts": {
    "badge": "ts-node scripts/badge.ts",
    "build": "gatsby build",
    "build:gh-pages": "gatsby build --prefix-paths",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "lint": "yarn lint:md && yarn lint:style && yarn lint:type-check",
    "lint:md": "markdownlint -i node_modules ./src/pages && stylelint ./src/pages/**/*.md",
    "lint:style": "eslint --ext .js,.jsx,.ts,.tsx ./src && stylelint ./src/**/*.css",
    "lint:type-check": "tsc --noEmit",
    "prelease": "standard-version --dry-run -s",
    "release": "standard-version -s",
    "serve": "gatsby serve",
    "start": "yarn develop",
    "start:watch": "concurrently \"yarn:develop\" \"yarn:test:watch\"",
    "test": "jest -o",
    "test:all": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@octokit/rest": "^18.11.4",
    "@reach/router": "^1.3.4",
    "antd": "^4.16.13",
    "classnames": "^2.3.1",
    "gatsby": "^3.14.1",
    "prismjs": "^1.25.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-spring": "^9.2.4",
    "tailwindcss": "^2.2.16",
    "typed.js": "^2.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@dg-scripts/eslint-config": "^1.1.3",
    "@dg-scripts/stylelint-config": "^1.1.3",
    "@octokit/types": "^6.31.3",
    "@react-spring/types": "^9.2.4",
    "@testing-library/dom": "^8.6.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.1",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.1",
    "@types/node-fetch": "^2.5.12",
    "@types/prismjs": "^1.16.6",
    "@types/reach__router": "^1.3.9",
    "@types/react": "^17.0.26",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@types/react-test-renderer": "^17.0.1",
    "autoprefixer": "^10.3.6",
    "babel-jest": "^27.2.4",
    "babel-preset-gatsby": "^1.14.0",
    "concurrently": "^6.2.2",
    "core-js": "^3.18.1",
    "eslint": "^7.32.0",
    "eslint-webpack-plugin": "^2.5.4",
    "gatsby-plugin-antd": "^2.2.0",
    "gatsby-plugin-catch-links": "^3.14.0",
    "gatsby-plugin-eslint": "^4.0.0",
    "gatsby-plugin-manifest": "^3.14.0",
    "gatsby-plugin-minify-html": "^1.0.4",
    "gatsby-plugin-nprogress": "^3.14.0",
    "gatsby-plugin-offline": "^4.14.0",
    "gatsby-plugin-postcss": "^4.14.0",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-robots-txt": "^1.6.10",
    "gatsby-plugin-sitemap": "^4.10.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.25",
    "gatsby-plugin-zopfli": "^2.0.0",
    "gatsby-remark-autolink-headers": "^4.11.0",
    "gatsby-remark-copy-linked-files": "^4.11.0",
    "gatsby-remark-prismjs": "^5.11.0",
    "gatsby-remark-smartypants": "^4.11.0",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-transformer-remark": "^4.11.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.4",
    "markdownlint-cli": "^0.28.1",
    "node-fetch": "^2.6.5",
    "postcss": "^8.3.8",
    "postcss-import": "^14.0.2",
    "prettier": "^2.4.1",
    "react-test-renderer": "^16.14.0",
    "standard-version": "^9.3.1",
    "stylelint": "^13.13.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "webpack": "^5.54.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sabertazimi/blog"
  },
  "bugs": {
    "url": "https://github.com/sabertazimi/blog/issues"
  }
}
