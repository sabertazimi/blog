{
  "name": "@sabertazimi/blog",
  "version": "2.13.0",
  "description": "A blog system based on React and Gatsby",
  "author": "sabertazimi",
  "license": "MIT",
  "keywords": [
    "gatsby"
  ],
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "badge": "ts-node scripts/badge.ts",
    "build": "gatsby build",
    "build:gh-pages": "gatsby build --prefix-paths",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "lint": "yarn lint:md && yarn lint:style && yarn lint:type-check",
    "lint:fix": "yarn lint:md:fix && yarn lint:style:fix",
    "lint:md": "eslint ./src/pages && stylelint ./src/pages/**/*.md && markdownlint ./src/pages",
    "lint:md:fix": "eslint ./src/pages --fix && stylelint ./src/pages/**/*.md --fix && markdownlint ./src/pages --fix",
    "lint:style": "eslint --ext .js,.jsx,.ts,.tsx ./src && stylelint ./src/**/*.css",
    "lint:style:fix": "eslint --ext .js,.jsx,.ts,.tsx ./src --fix && stylelint ./src/**/*.css --fix",
    "lint:type-check": "tsc --noEmit",
    "prelease": "standard-version --dry-run -s",
    "release": "standard-version -s",
    "serve": "gatsby serve",
    "start": "yarn develop",
    "start:watch": "concurrently \"yarn:develop\" \"yarn:test:watch\"",
    "test": "jest -o",
    "test:all": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@octokit/rest": "^18.12.0",
    "@reach/router": "^1.3.4",
    "antd": "^4.19.5",
    "classnames": "^2.3.1",
    "gatsby": "^4.11.2",
    "prismjs": "^1.27.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-helmet": "^6.1.0",
    "react-spring": "^9.4.4",
    "tailwindcss": "^3.0.23",
    "typed.js": "^2.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@dg-scripts/eslint-config": "^5.8.1",
    "@dg-scripts/stylelint-config": "^5.8.1",
    "@octokit/types": "^6.34.0",
    "@react-spring/types": "^9.4.4",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.0.4",
    "@types/jest": "^27.4.1",
    "@types/jest-axe": "^3.5.3",
    "@types/node": "^16.11.26",
    "@types/prismjs": "^1.26.0",
    "@types/reach__router": "^1.3.10",
    "@types/react": "^18.0.1",
    "@types/react-dom": "^18.0.0",
    "@types/react-helmet": "^6.1.5",
    "@types/react-test-renderer": "^18.0.0",
    "autoprefixer": "^10.4.4",
    "babel-jest": "^27.5.1",
    "babel-preset-gatsby": "^2.11.1",
    "concurrently": "^7.1.0",
    "core-js": "^3.21.1",
    "eslint": "^8.13.0",
    "eslint-webpack-plugin": "^3.1.1",
    "gatsby-plugin-antd": "^2.2.0",
    "gatsby-plugin-catch-links": "^4.11.0",
    "gatsby-plugin-eslint": "^4.0.2",
    "gatsby-plugin-manifest": "^4.11.1",
    "gatsby-plugin-minify-html": "^1.0.4",
    "gatsby-plugin-nprogress": "^4.11.0",
    "gatsby-plugin-offline": "^5.11.1",
    "gatsby-plugin-postcss": "^5.11.0",
    "gatsby-plugin-react-helmet": "^5.11.0",
    "gatsby-plugin-robots-txt": "^1.7.0",
    "gatsby-plugin-sitemap": "^5.11.1",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.27",
    "gatsby-plugin-zopfli": "^2.0.0",
    "gatsby-remark-autolink-headers": "^5.11.1",
    "gatsby-remark-copy-linked-files": "^5.11.0",
    "gatsby-remark-prismjs": "^6.11.0",
    "gatsby-remark-smartypants": "^5.11.0",
    "gatsby-source-filesystem": "^4.11.1",
    "gatsby-transformer-remark": "^5.11.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-axe": "^6.0.0",
    "markdownlint-cli": "^0.31.1",
    "postcss": "^8.4.12",
    "postcss-import": "^14.1.0",
    "prettier": "^2.6.2",
    "react-test-renderer": "^18.0.0",
    "standard-version": "^9.3.2",
    "stylelint": "^14.6.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "undici": "^5.0.0",
    "webpack": "^5.72.0"
  },
  "dependenciesMeta": {
    "@react-spring/core": {
      "built": false
    }
  },
  "resolutions": {
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sabertazimi/blog"
  },
  "bugs": {
    "url": "https://github.com/sabertazimi/blog/issues"
  }
}
